{
  "consumer": {
    "name": "movie-consumer"
  },
  "provider": {
    "name": "movie-provider"
  },
  "interactions": [
    {
      "description": "a request to list all movies",
      "request": {
        "method": "GET",
        "path": "/api/movies",
        "headers": {}
      },
      "response": {
        "status": 200,
        "body": [
          {
            "id": 1,
            "name": "Movie 1",
            "year": 2008
          },
          {
            "id": 2,
            "name": "Movie 2",
            "year": 2008
          }
        ]
      }
    },
    {
      "description": "should delete a movie",
      "request": {
        "method": "DELETE",
        "path": "/api/movie/1",
        "headers": {}
      },
      "response": {
        "status": 200,
        "body": "deleted"
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    },
    "client": {
      "name": "pact-msw-handlers",
      "version": "1.0.0"
    }
  }
}